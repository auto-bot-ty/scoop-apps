    "homepage": "https://www.jetbrains.com/idea/",
    "license": {
        "identifier": "Proprietary",
        "url": "https://www.jetbrains.com/store/license.html"
    },
    "url": [
        "https://download.jetbrains.com/idea/ideaIU-212.4746.52.win.zip",
        "https://raw.githubusercontent.com/Ash258/Scoop-JetBrains/master/scripts/portable.ps1"
    ],
    "hash": [
        "1a0c566e8cd6a0fb5251b76297f0790a05d77341fb48a5b2eb090deea20abc21",
        "5b43fc8a57d7d8d519071a50849a5db92c90ecdf56b64ac57f15fdae5b4f0572"
    ],
    "extract_to": "IDE",
    "installer": {
        "script": [
            "& \"$dir\\portable.ps1\" $dir $persist_dir",
            "Remove-Item \"$dir\\IDE\\`$*\", \"$dir\\portable.ps1\" -Force -Recurse"
        ]
    },
    "architecture": {
        "64bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\idea64.exe",
                    "JetBrains\\IntelliJ IDEA Ultimate (Release Candidate)"
                ]
            ]
        },
        "32bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\idea.exe",
                    "JetBrains\\IntelliJ IDEA Ultimate (Release Candidate)"
                ]
            ]
        }
    },
    "persist": [
        "IDE\\bin\\idea.properties",
        "profile"
    ],
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=IIU&latest=true&platform=zip&type=rc",
        "regex": "version\":\"(?<ver>[\\d.]+)\".*\"build\":\"(?<build>[\\d.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/idea/ideaIU-$preReleaseVersion.win.zip",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
