{
    "description": "Official General-Availability Release of OpenJDK",
    "homepage": "https://jdk.java.net/",
    "version": "16-36",
    "license": "GPL-2.0-only WITH Classpath-exception-2.0",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/GA/jdk16/7863447f0ab643c585b9bdebf67c69db/36/GPL/openjdk-16_windows-x64_bin.zip",
            "hash": "a78bdeaad186297601edac6772d931224d7af6f682a43372e693c37020bd37d6"
        }
    },
    "extract_dir": "jdk-16",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://jdk.java.net/16/",
        "re": "/(?<type>early_access|GA)/(?<path>jdk(?<major>[\\d.]+)(?:.*)?/(?<build>[\\d]+)(?:/GPL|/binaries)?)/(?<file>openjdk-(?<version>[\\d.]+)(?<ea>-ea)?(?:\\+[\\d]+)?_windows-x64_bin.(zip|tar.gz))",
        "replace": "${version}-${build}${ea}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/$matchType/$matchPath/$matchFile"
            }
        },
        "hash": {
            "url": "$url.sha256"
        },
        "extract_dir": "jdk-$matchVersion"
    }
}
