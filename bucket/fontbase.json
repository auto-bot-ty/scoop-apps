{
    "version": "2.15.2",
    "description": "Font manager",
    "homepage": "https://fontba.se",
    "license": {
        "identifier": "Freeware",
        "url": "https://fontba.se/legal/terms"
    },
    "architecture": {
        "64bit": {
            "url": "https://releases.fontba.se/win/FontBase-2.15.2.exe#/dl.7z",
            "hash": "sha512:417de73c7193fcb6e0629c1bb4b5780cce5cbf15a21352b6a452505ba5f809fad1f3065d08465109e8d79749a1367ba78135ec868df07bbbd96f472495e9e0a0",
            "pre_install": [
                "Expand-7ZipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\uninstall*\" -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "FontBase.exe",
            "FontBase"
        ]
    ],
    "checkver": {
        "url": "https://fontba.se/updates",
        "regex": "([\\d.]+)</a"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://releases.fontba.se/win/FontBase-$version.exe#/dl.7z",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "regex": "sha512:\\s+$base64"
                }
            }
        }
    }
}
